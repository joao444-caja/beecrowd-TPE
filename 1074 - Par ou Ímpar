#include <stdio.h>
int main() {
    int N;
    scanf("%d", &N); 
    for (int i = 0; i < N; i++) {
        int X;
        scanf("%d", &X); 
        if (X == 0) {
            printf("NULL\n");
        } else {
            // Verifica se é PAR ou ÍMPAR
            if (X % 2 == 0) {
                printf("EVEN ");
            } else {
                printf("ODD ");
            }

            // Verifica se é POSITIVO ou NEGATIVO
            if (X > 0) {
                printf("POSITIVE\n");
            } else { // X < 0
                printf("NEGATIVE\n");
            }
        }
    }
    return 0;
}